<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--圆环进度-->
    <declare-styleable name="CircleProgressView">
        <!--背景环宽度百分比-->
        <attr name="cr_widthRatio" format="float" />
        <!--进度环百分比-->
        <attr name="cr_progressWidthRatio" format="float" />
        <!--进度动画持续时间-->
        <attr name="cr_animationTime" format="integer" />
        <!--进度值 0-1f-->
        <attr name="cr_progress" format="float" />
        <!--画布旋转角度-->
        <attr name="cr_rotateAngle" format="integer" />
        <!--进度条颜色,可以渐变,逗号隔开-->
        <attr name="cr_progressColor" format="string" />
        <!--环背景色-->
        <attr name="cr_behindColor" format="color" />
    </declare-styleable>
</resources>
